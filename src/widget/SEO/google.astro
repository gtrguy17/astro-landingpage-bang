---
let title = 'Aplikasi Restoran';
let description = 'Solusi all in one website restoran dan aplikasi online restoran cafe rumah makan plus android app dalam satu aplikasi untuk bisnis mu.';
let cover = 'https://media.istockphoto.com/vectors/online-mobile-application-food-delivery-concept-bakery-shop-owner-vector-id1136519512?k=20&m=1136519512&s=612x612&w=0&h=xBqQG-7rYdE_7hF2o5LODC_7NasZMPUzVVSCFp5Ns_8=';
let facebook = 'https://www.facebook.com/mesinkasircomplete';
let twitter = 'https://twitter.com/hockeycomp';
let twitteruser = '@hockeycomp';
let author = 'https://axcora.com';
let type = 'website';
let icon = 'https://astro.build/assets/press/logomark-light.svg';
let google = 'ebvLr84CVTSpjtrusz7g7RB21J384B2SJOGrau8jl04';
---
<meta name="google-site-verification" content={google} />
<title>{title}</title>
<meta content={title} property='og:title'/>
<meta content={title} name='facebook:title'/>
<meta content={title} name='twitter:title'/>
<meta content={title} property='og:site_name'/>
<meta content={title} name='keywords'/>
<meta content={description} name="content"/>
<meta content={description} property='facebook:description'/>
<meta content={description} name='twitter:description'/>
<meta content={author} property='og:author'/>
<meta content={facebook} property='facebook:author'/>
<meta content={twitter} property='twitter:author'/>
<meta content={twitteruser} name='twitter:site'/>
<meta content={type} property='og:type'/>
<meta content='summary_large_image' name='twitter:card'/>
<meta content={cover} name='facebook:image'/>
<meta content={cover} name='twitter:image'/>
<link rel="icon" type="image/x-icon" href={icon} />
<meta content='index, follow' name='robots'/>
